{
  "name": "bakong-notification-services",
  "version": "0.0.1",
  "description": "Bakong Notification Service for announcement",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf dist || rmdir /s /q dist || true",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json && node fix-paths.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:staging": "cross-env NODE_ENV=staging nest start",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "build:dev": "cross-env NODE_ENV=development nest build",
    "build:staging": "cross-env NODE_ENV=staging nest build",
    "build:prod": "cross-env NODE_ENV=production nest build",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --dataSource ./src/ormconfig.ts",
    "remove-duplicates": "ts-node -r tsconfig-paths/register scripts/remove-duplicate-images.ts",
    "fix-null-fileid": "ts-node -r tsconfig-paths/register scripts/fix-null-fileid.ts",
    "start:dev-tunnel": "NODE_ENV=development npx localtunnel --port 4002 --subdomain bakong-notification-services"
  },
  "dependencies": {
    "@bakong/shared": "file:../packages/shared",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^2.2.0",
    "braces": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "extend-shallow": "^3.0.2",
    "fill-range": "^7.1.1",
    "firebase-admin": "^11.3.0",
    "helmet": "^6.0.1",
    "iconv-lite": "^0.4.24",
    "is-extendable": "^1.0.1",
    "is-number": "^7.0.0",
    "json5": "^2.2.3",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.8",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "safer-buffer": "^2.1.2",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "to-object": "^0.0.1",
    "typeorm": "^0.3.10",
    "util-deprecate": "^1.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.27.1",
    "@babel/core": "^7.28.4",
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/template": "^7.27.2",
    "@babel/traverse": "^7.28.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tree-kill": "^1.2.2",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!node_modules",
      "!**/*.entity.ts",
      "!**/i18n",
      "!**/constant.ts",
      "!**/ormconfig.ts",
      "!**/*.module.ts",
      "!**/*.dto.ts",
      "!**/*.enum.ts",
      "!**/*.interceptor.ts",
      "!**/*.guard.ts",
      "!**/*.pipe.ts",
      "!**/*.filter.ts",
      "!**/*.strategy.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.nbc.gov.kh/bakong-projects/bakong-notification-service.git"
  },
  "keywords": [],
  "type": "commonjs"
}
