t timestamp [not null] [default: now()] +updatedAt timestamp [not null] [default: now()] +} ++Table user { +id int [not null] [default: \x07uto] +username varchar(255) [not null] +password varchar(255) [not null] +displayName varchar(255) [not null] +failLoginAttempt int [not null] [default: 0] +role USER-DEFINED [not null] [default: 'NORMAL_USER'::user_role_enum] +createdAt timestamp [not null] [default: now()] +updatedAt timestamp [not null] [default: now()] +deletedAt timestamp +} ++Ref: template.categoryTypeId > category_type.id +Ref: template_translation.imageId > image.fileId+